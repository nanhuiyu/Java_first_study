//1，导包
import java.util.Scanner;
public class 键盘录入 {
    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        System.out.println("please enter in");
        int a=scan.nextInt();
        int b=scan.nextInt();
        System.out.println(a+b);
        scan.close();
    }
}
